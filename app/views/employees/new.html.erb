<h1> Create Employee </h1>

<%= form_for @employee do |f| %>
  <%= f.label :first_name %>
  <br>
  <%= f.text_field :first_name %>
  <br>
  <%= f.label :last_name %>
  <br>
  <%= f.text_field :last_name %>
  <br>
  <%= f.label :alias %>
  <br>
  <%= f.text_field :alias %>
  <br>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title %>
  <br>
  <%= f.label :office %>
  <br>
  <%= f.text_field :office %>
  <br>
  <%= f.label 'Dog:' %>
  <br>
  <%= f.collection_select :dog_id, @dogs, :id, :name %>
  <br>
  <%= f.submit %>
  <% end %>
